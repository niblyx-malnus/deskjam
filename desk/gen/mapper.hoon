/-  *jam-desk
:-  %say
|=  $:  [now=@da eny=@uvJ bec=beak]
        [[=desk files=(unit (list path)) ~] ~]
    ==
:-  %mapp
|^
(~(gas by *mapp) (turn ?~(files all-files u.files) mage))
++  bek  (en-beam bec(q desk) ~)
++  all-files  .^((list path) %ct bek)
++  mage
  |=  file=path
  ^-  [path page:clay]
  ?~  file  !!
  :-  file
  :-  (rear file)
  ~|  [%missing-source-file desk file]
  .^(* %cx (weld bek file))
--
